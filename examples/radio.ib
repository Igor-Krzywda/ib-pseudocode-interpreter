method frequency_range(ARR)
    LEN = ARR.length() - 1
    MIN = ARR[0]
    MAX = ARR[0]
    loop from I from 1 to LEN
        if MIN > ARR[I]
            MIN = ARR[I]
        if MAX < ARR[I]
            MAX = ARR[I]
    end loop
    return MAX - MIN
end method

